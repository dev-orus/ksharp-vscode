{
  "name": "wisp",
  "displayName": "Wisp",
  "description": "The vscode extension for Wisp",
  "repository": {
    "url": "https://github.com/dev-orus/wisp-vscode"
  },
  "publisher": "Klesti",
  "version": "0.0.3",
  "icon": "logo.png",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "wisp",
        "icon": {
          "dark": "icon.png",
          "light": "icon.png"
        },
        "aliases": [
          "Wisp",
          "wisp"
        ],
        "extensions": [
          ".ws"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "wisp",
        "scopeName": "source.wisp",
        "path": "./syntaxes/wisp.tmLanguage.json"
      }
    ]
  }
}
